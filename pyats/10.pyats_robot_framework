pip install pyats.robot
# RobotFramework is installed as a component under pyATS, with the pyats[full] package install

(majid) majid@majid-ubuntu:~/devnet/pyats$ pip freeze | grep robot
genie.libs.robot==24.11
pyats.robot==24.11
robotframework==7.1.1

(majid) majid@majid-ubuntu:~/devnet/pyats$ robot --version
Robot Framework 7.1.1 (Python 3.12.3 on linux)
(majid) majid@majid-ubuntu:~/devnet/pyats$

