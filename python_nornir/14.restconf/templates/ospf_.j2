{
    "Cisco-IOS-XE-ospf:ospf": {
        {% for ospf in host.hdata.ospf %}
        "process-id": [
            {
                "id": "{{ ospf.process_id }}",
                {% for networks in ospf.networks %}
                "network": [
                    {"ip": "{{ networks.network }}", "wildcard": "{{ networks.wildmask }}", "area": "{{ networks.area }}"}
                ],
                {% endfor %}
                "router-id": "{{ ospf.router_id }}"
            }
        ]
    {% endfor %}
    }
}
